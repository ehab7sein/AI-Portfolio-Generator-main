# API Keys Configuration Template
# Copy this file to .env and replace with your actual keys

# Gemini API Key
# Get your key from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Supabase Configuration
# Get from: https://app.supabase.com/project/_/settings/api
VITE_SUPABASE_PROJECT_ID=your_project_id
VITE_SUPABASE_PUBLISHABLE_KEY=your_supabase_anon_key_here
VITE_SUPABASE_URL=https://your-project.supabase.co

# OpenRouter (Optional)
# Get from: https://openrouter.ai/keys
VITE_OPENROUTER_API_KEY=your_openrouter_key_here
VITE_OPENROUTER_MODEL=xiaomi/mimo-v2-flash:free

# Azure OpenAI (Optional)
# Get from: https://portal.azure.com
AZURE_OPENAI_ENDPOINT=https://your-resource.openai.azure.com/
AZURE_OPENAI_API_KEY=your_azure_openai_key_here
AZURE_OPENAI_DEPLOYMENT=gpt-4

# Azure Cognitive Services Speech API (Optional)
# Get from: https://portal.azure.com
VITE_AZURE_SPEECH_KEY=your_azure_speech_key_here
VITE_AZURE_SPEECH_REGION=your_region

# Server Port
PORT=3000

# IMPORTANT: Never commit your actual .env file to Git!
# This is just a template. Your real keys should be in .env (which is gitignored)